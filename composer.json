{
    "name": "siskeudes-lite/web-app",
    "description": "Sistem Keuangan Desa Berbasis Web menggunakan CodeIgniter 4",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "app/Config/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "support": {
        "source": "https://github.com/siskeudes-lite/web-app"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
